<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Authors>Raphael Nöldner</Authors>
    <RootNamespace>CsvTools</RootNamespace>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Copyright>Copyright © 2017</Copyright>
    <ProductName>CsvTools</ProductName>
    <OutputType>Library</OutputType>
    <ApplicationIcon />
    <StartupObject />
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <AssemblyVersion>1.6.6.458</AssemblyVersion>
    <FileVersion>1.6.6.458</FileVersion>
    <Version>1.6.6.458</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <NeutralLanguage>en-US</NeutralLanguage>
    <TargetFramework>net48</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="CsvFileReaderAsync.cs" />
    <Compile Remove="DataTableReaderAsync.cs" />
    <Compile Remove="SettingsReadOnly\SettingReadOnly.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="DateTimeFormats.txt" />
    <None Include="LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="DateTimeFormats.txt" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="JetBrains.Annotations" Version="2020.1.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="NLog" Version="4.7.2" />
    <PackageReference Include="Pri.LongPath" Version="2.0.53" />
    <PackageReference Include="Ude.NetStandard" Version="1.2.0" />
  </ItemGroup>
</Project>