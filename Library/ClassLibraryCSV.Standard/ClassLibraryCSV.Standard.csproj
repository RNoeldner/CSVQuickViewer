<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>CsvTools</RootNamespace>
    <Authors>Raphael Nöldner</Authors>
    <Company></Company>
    <Product>CSVTools</Product>
    <AssemblyVersion>1.6.4.391</AssemblyVersion>
    <FileVersion>1.6.4.391</FileVersion>
	  <TargetFramework>netstandard2.0</TargetFramework>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\ClassLibraryCSV\**\*.cs" />
    <EmbeddedResource Include="..\ClassLibraryCSV\**\*.txt" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="..\**\ClassLibraryCSV.AssemblyInfo.cs" />
    <Compile Remove="obj\**" />
    <EmbeddedResource Remove="..\ClassLibraryCSV\obj\**" />
    <EmbeddedResource Remove="obj\**" />
    <None Remove="obj\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Ben.Demystifier" Version="0.1.6" />
    <PackageReference Include="JetBrains.Annotations" Version="2020.3.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="Serilog.Sinks.File" Version="4.1.0" />
    <PackageReference Include="SharpZipLib" Version="1.3.1" />
    <PackageReference Include="UTF.Unknown" Version="2.3.0" />
  </ItemGroup>
</Project>