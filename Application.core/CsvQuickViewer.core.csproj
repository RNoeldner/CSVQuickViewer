<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <ApplicationIcon>..\Application\csv_text.ico</ApplicationIcon>
    <ApplicationManifest>app1.manifest</ApplicationManifest>
  </PropertyGroup>
 
  <ItemGroup>
    <Compile Include="..\Application\**\*.cs" />
    <EmbeddedResource Include="..\Application\**\*.resx" />
    <EmbeddedResource Include="..\Application\**\*.ico" />
  </ItemGroup>
 
  <ItemGroup>
    <Compile Remove="..\Application\Properties\AssemblyInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="log4net" Version="2.0.8" />
    <PackageReference Include="Pri.LongPath">
      <Version>2.0.47</Version>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Library\ClassLibraryCSV\ClassLibraryCSV.csproj" />
    <ProjectReference Include="..\Library\WinFormControls\WinFormControls.csproj" />
  </ItemGroup>
</Project>