<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net472</TargetFramework>
		<UseWindowsForms>true</UseWindowsForms>
		<LangVersion>8.0</LangVersion>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<GenerateTargetFrameworkAttribute>false</GenerateTargetFrameworkAttribute>
		<RootNamespace>CsvTools.Tests</RootNamespace>
		<TestProjectType>UnitTest</TestProjectType>
		<Nullable>enable</Nullable>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DefineConstants>TRACE;Windows</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<DefineConstants>Windows</DefineConstants>
	</PropertyGroup>
	<ItemGroup>
	  <Compile Remove="WinAppLoggingTests.cs" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="MSTest.TestAdapter" Version="2.2.10" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\..\..\FileValidator\Library\ClassLibraryValidator\CsvTools.ClassLibraryValidator.csproj" />
		<ProjectReference Include="..\..\Library\WinFormControls\CsvTools.WinFormControlsLibrary.csproj" />
		<ProjectReference Include="..\UnitTestBasic\CsvTools.UnitTestBasic.csproj" />
	</ItemGroup>
	<ItemGroup>
		<Reference Include="FastColoredTextBox">
			<HintPath>..\..\Library\DLL\FastColoredTextBox.dll</HintPath>
		</Reference>
	</ItemGroup>
	<ItemGroup>
		<None Update="TestFiles\*.txt">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</None>
	</ItemGroup>
</Project>